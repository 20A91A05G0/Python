def prime(p):
    import math
    s=int(math.sqrt(p))
    c=0
    for i in range(1,s+1):
        if p%i==0:
            c+=1
    if c>1:
        print(p,'is not a prime')
    else:
        print(p,'is a prime')
p=int(input(' enter a number'))
prime(p)
    
#output
enter a number 7
7 is a prime
enter a number 6
6 is not a prime
